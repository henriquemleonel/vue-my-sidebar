{
  "name": "vue-admin-sidebar",
  "description": "A Vue.js admin sidbar component",
  "version": "1.0.0",
  "author": "Md MostaFizur Rahaman",
  "license": "MIT",
  "main": "dist/vue-admin-sidebar.ssr.js",
  "browser": "dist/vue-admin-sidebar.esm.js",
  "module": "dist/vue-admin-sidebar.esm.js",
  "unpkg": "dist/vue-admin-sidebar.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MdMostaFizurRahaman/vue-admin-sidebar.git"
  },
  "keywords": [
    "vue",
    "vue-component",
    "vue-sidebar",
    "vue-sidebar-component",
    "vue-admin-sidebar",
    "sidebar",
    "admin-sidebar"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^2.0.8",
    "node-sass": "^6.0.0",
    "sass-loader": "^10.2.0",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.6",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
